#include <iostream>

using namespace std;

int main()
{
  int even [10];
  cout<<"please enter 10 even number"<<endl;
  
  for (int i=0;i<10;i++)
   cin>>even[i];
 
     cout<<"\nThe second element is" <<even[1]<<endl;
     cout<<"\nThe fourth element is" <<even[3]<<endl;
     even[4]=even[1]*even[3];
     cout<<"The fifth element now is :"<<even[4]<<endl;
     cout<<"The number in array even now are:"<<endl;
     for (int i=0;i<10;i++)
     cout<<even[i]<<" "<<endl;
     
  return 0;
}
